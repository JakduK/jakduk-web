<!DOCTYPE html>
<!--[if IE 9]><html lang="{{locale}}" class="ie9" ng-app="jakdukApp"><![endif]-->
<!--[if !IE]><!--><html lang="{{locale}}" ng-app="jakdukApp"><!--<![endif]-->
{{#if baseHref}}
  <base href="{{baseHref}}">
{{/if}}
  <head>
  <title>{{#if preTitle}}{{{preTitle}}} &middot; {{/if}}{{{TITLE title}}}</title>
  {{> head_common}}
  {{#if headPage}}
    {{> (lookup . 'headPage')}}
  {{/if}}
  </head>

  <body class="{{bodyClass}}">
    {{{body}}}
    {{#if gaAccount}}
    <script type="text/javascript">
      (function (i, s, o, g, r, a, m) {i['GoogleAnalyticsObject'] = r;i[r] = i[r] || function () {(i[r].q = i[r].q || []).push(arguments)}, i[r].l = 1 * new Date();a = s.createElement(o),m = s.getElementsByTagName(o)[0];a.async = 1;a.src = g;m.parentNode.insertBefore(a, m)})(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
      (function () {
        ga('create', '{{gaAccount}}', '{{userInfo.username}}' ? {'userId': '{{userInfo.username}}'} : 'auto');
        ga('send', 'pageview');
      }());
    </script>
    {{/if}}
  </body>
</html>
